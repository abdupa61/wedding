{
  "version": 2,
  "builds": [
    { "src": "api/uploadthing.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/uploadthing", "dest": "api/uploadthing.js" }
  ],
  "functions": {
    "api/uploadthing.js": {
      "maxDuration": 30
    }
  },
  "env": {
    "UPLOADTHING_SECRET": "@uploadthing-secret",
    "UPLOADTHING_APP_ID": "@uploadthing-app-id"
  }
}
